# .env.example - Bu dosyayı kopyalayıp .env olarak kaydedin ve kendi bilgilerinizi girin

# Django Secret Key
SECRET_KEY=your-secret-key-here

# Debug Mode (production'da False olmalı)
DEBUG=True

# Allowed Hosts (production'da domain adınızı ekleyin, virgülle ayırın)
ALLOWED_HOSTS=localhost,127.0.0.1,yourdomain.pythonanywhere.com

# Email Configuration
EMAIL_HOST_USER=your-email@gmail.com
EMAIL_HOST_PASSWORD=your-app-password
ADMIN_EMAIL=your-email@gmail.com

# Telegram Bot
TELEGRAM_BOT_TOKEN=your-telegram-bot-token
TELEGRAM_CHAT_ID=your-telegram-chat-id
